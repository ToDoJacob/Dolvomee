<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.yedam.dolvomee.service.cartlist.CartListMapper">

	<select id="selectCartList" resultType="co.yedam.dolvomee.service.cartlist.CartListVO">
		 select * from Cartlist
	</select>
	
	<select id="selectCart" resultType="co.yedam.dolvomee.service.cartlist.CartListVO">
	 	select * from Cartlist where usersEmail = #{usersEmail}
	</select>

</mapper>